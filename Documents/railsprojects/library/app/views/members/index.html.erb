<p id="notice"><%= notice %></p>

<h1>Displaying all members</h1><br>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.address %></td>
        <td><%= member.email %></td>
        <td><%= member.phone %></td>
        <td><%= link_to image_tag("show.jpg", :size=>"20x20"), member %></td>
        <td><%= link_to image_tag("edit.jpg", :size=>"20x20"), edit_member_path(member) %></td>
        <td><%= link_to image_tag("destroy.jpg", :size=>"20x20"), member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
